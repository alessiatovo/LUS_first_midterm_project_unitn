#Unigram
U00:%x[0,0]
U01:%x[-2,0]
U02:%x[-1,0]
U03:%x[1,0]
U04:%x[2,0]
U05:%x[0,1]
U06:%x[0,0]/%x[0,1]
U07:%x[0,0]/%x[0,2]
U08:%x[3,0]
U09:%x[0,3]
U10:%x[0,4]
U11:%x[-1,3]/%x[-1,4]
U12:%x[1,3]/%x[1,4]
U13:%x[0,1]/%x[0,2]/%x[0,3]
U14:%x[0,1]/%x[0,2]/%x[0,4]
U15:%x[0,3]/%x[0,4]

#Bigram
B
B00:%x[-2,0]
B01:%x[-1,0]
B02:%x[0,0]
B03:%x[1,0]
B04:%x[2,0]
B05:%x[0,1]
B06:%x[0,3]
B07:%x[0,4]